layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Mark Mims</title>
 <link href="http://blog.markmims.com/atom.xml" rel="self"/>
 <link href="http://blog.markmims.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://blog.markmims.com/</id>
 <author>
   <name>Mark Mims</name>
   <email>mark.mims@gmail.com</email>
 </author>
 {% for post in site.categories.cloud limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://al3x.net{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://blog.markmims.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
   <author>
     <name>Mark Mims</name>
     <uri>http://markmims.com/</uri>
   </author>
 </entry>
 {% endfor %}
</feed>
